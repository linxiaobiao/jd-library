page {
    min-height: 100%;
    background: #efeff4;
}

.wrap {
    width: 100%;
    box-sizing: border-box;
    font-family: Arial,Helvetica,sans-serif;
}

.distribution-box {
    padding-left: 20rpx;
    padding-top: 30rpx;
    width: 100%;
    box-sizing: border-box;
    background: #fff;
    border-bottom: 1rpx solid #e7e7e7;
    border-top: 1rpx solid #e7e7e7;
    margin-bottom: 14rpx;
}

.distribution-box .distribution-top {
    width: 100%;
    height: 50rpx;
    font-size: 30rpx;
    display: flex;
    align-items: center;
}

.distribution-box .che {
    width: 36rpx;
    height: 28rpx;
    margin-right: 20rpx;
    background: url(data:image/png;base64,);
    background-size: 100% 100%;
}

.distribution-box .distribution-btn-box {
    width: 100%;
    padding-top: 20rpx;
    padding-bottom: 30rpx;
    display: flex;
    align-items: center;
}

.distribution-box .distribution-btn {
    height: 70rpx;
    width: auto;
    border: 2rpx solid #bdbdbd;
    border-radius: 7rpx;
    font-size: 30rpx;
    display: flex;
    align-items: center;
    padding: 0 18rpx;
    margin-right: 20rpx;
}

.distribution-box .distribution-btn.checked {
    border: 2rpx solid #f23030;
    color: #f23030;
}

.time-check-box {
    width: 100%;
    box-sizing: border-box;
    padding: 20rpx 0;
    display: flex;
    height: auto;
    border-top: 1rpx solid #e7e7e7;
}

.time-check-box .check-l {
    box-sizing: border-box;
    padding-left: 8rpx;
    flex: 1;
}

.time-check-box .check-t {
    width: 100%;
    height: 54rpx;
    line-height: 54rpx;
    font-size: 30rpx;
    display: block;
    overflow: hidden;
}

.time-check-box .check-b {
    width: 100%;
    height: 48rpx;
    line-height: 48rpx;
    font-size: 30rpx;
    display: block;
    overflow: hidden;
}

.time-check-box .check-r {
    width: 70rpx;
    display: flex;
    align-items: center;
}

.time-check-box .time-icon {
    width: 51rpx;
    height: 49rpx;
    background: url(data:image/png;base64,);
    background-size: 100% 100%;
}

.alert-box {
    width: 74%;
    height: 564rpx;
    position: fixed;
    left: 50%;
    top: 50%;
    background: #fff;
    margin: -282rpx 0 0 -37%;
    border-radius: 10rpx;
    z-index: 300;
    border: 1rpx solid #e7e7e7;
    display: none;
}

.alert-mask {
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.5);
    position: fixed;
    left: 0;
    top: 0;
    z-index: 200;
    display: none;
}

.alert-title {
    width: 100%;
    height: 74rpx;
    padding-top: 4rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 36rpx;
}

.alert-content-box {
    display: block;
    height: 380rpx;
    box-sizing: border-box;
    margin: 0 45rpx;
    background: url(data:image/png;base64,);
    background-repeat: repeat;
    background-size: 4rpx 82rpx;
    overflow: hidden;
    display: -webkit-flex;
    display: flex;
}

.alert-scroll-view-l {
    width: 50%;
    height: 380rpx;
    background: rgba(255,255,255,0.01);
    overflow: hidden;
}

.alert-scroll-view-l view {
    width: 100%;
    height: 82rpx;
    display: flex;
    align-items: center;
    font-size: 28rpx;
    color: #888;
}

.alert-scroll-view-l .check {
    color: #f23030;
}

.alert-scroll-view-r {
    width: 50%;
    height: 380rpx;
    background: rgba(255,255,255,0.01);
    overflow: hidden;
}

.alert-scroll-view-r view {
    width: 100%;
    height: 82rpx;
    display: flex;
    align-items: center;
    font-size: 28rpx;
    color: #888;
    box-sizing: border-box;
    padding-left: 54rpx;
}

.alert-scroll-view-r .check {
    color: #f23030;
}

.alert-scroll-c {
    width: 100%;
    height: 380rpx;
    background: rgba(255,255,255,0.01);
    overflow: hidden;
}

.alert-scroll-c view {
    width: 100%;
    height: 82rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 28rpx;
    color: #888;
}

.alert-scroll-c .check {
    color: #09bb07;
}

.alert-bttom-box {
    width: 100%;
    height: 106rpx;
    display: flex;
    box-sizing: border-box;
    border-top: 1rpx solid #e1e2e3;
}

.alert-btn {
    width: 50%;
    height: 100%;
    overflow: hidden;
    box-sizing: border-box;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 36rpx;
}

.alert-btn-green {
    color: #09bb07;
    border-left: 1rpx solid #e1e2e3;
    height: 100%;
}

.sub-btn-wrap {
    width: 100%;
    margin-top: 20rpx;
    padding: 0 30rpx 20rpx 30rpx;
    box-sizing: border-box;
}

.sub-btn-wrap .sub-btn {
    border-radius: 12rpx;
    background: #f23030;
    height: 90rpx;
    box-sizing: border-box;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 36rpx;
    color: #fff;
}

.product-multiple-info {
    padding: 24rpx 0 20rpx 0;
}

.scroll-img-wrap {
    width: 100%;
    height: 158rpx;
}

.prod-img-list {
    width: auto;
    height: 190rpx;
    overflow: hidden;
}

.prod-item-wrap {
    width: 158rpx;
    height: 158rpx;
    font-size: 0;
    display: inline-block;
    margin-right: 20rpx;
}

.prod-img-wrap {
    width: 158rpx;
    height: 158rpx;
    position: relative;
}

.product-img {
    width: 158rpx;
    height: 158rpx;
    border: 1rpx solid #f2f2f2;
    border-radius: 8rpx;
    box-sizing: border-box;
}