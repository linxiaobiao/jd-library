@import "../../components/searchBar/searchBar.wxss";

@import "../../components/searchTab/searchTab.wxss";

view,text {
    font-family: Arial,Helvetica,sans-serif;
    font-family: "SimHei","Helvetica Neue",Arial,"Droid Sans",sans-serif;
}

.content {
    position: relative;
    background-color: #f0f2f5;
    z-index: 10;
}

.product-list {
    background-color: #fff;
}

.product-list-top {
    margin-top: 76rpx;
}

.product-item {
    display: flex;
    width: 100%;
    height: 248rpx;
    justify-content: flex-start;
}

.product-image {
    width: 200rpx;
    height: 200rpx;
    margin: 28rpx 20rpx 20rpx 20rpx;
    position: relative;
}

.product-image .pimage {
    background-size: 211rpx 210rpx;
    background-image: url(data:image/png;base64,);
}

.product-image .product-ext {
    position: absolute;
    width: 100%;
    height: 30rpx;
    line-height: 30rpx;
    bottom: 0;
    left: 0;
    font-size: 24rpx;
    text-align: center;
    background-color: rgba(0,0,0,0.6);
    color: #fff;
}

.product-image image {
    height: 200rpx;
    max-width: 200rpx;
}

.product-info {
    flex: 1;
    height: 220rpx;
    margin-top: 28rpx;
    border-bottom: 1rpx solid #e3e5e9;
    padding-right: 50rpx;
    position: relative;
    box-sizing: border-box;
}

.product-title {
    font-size: 28rpx;
    color: #232326;
    overflow: hidden;
    width: 100%;
    height: 80rpx;
    line-height: 40rpx;
    margin-bottom: 42rpx;
}

.limit-lines {
    overflow: hidden;
    text-overflow: ellipsis;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    vertical-align: middle;
    display: -webkit-box;
    -webkit-box-pack: center;
    -webkit-box-align: start;
    word-break: break-all;
}

.product-recommend {
    font-size: 22rpx;
    color: #999;
    height: 30rpx;
    line-height: 30rpx;
    margin-top: 10rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.product-author {
    font-size: 22rpx;
    height: 30rpx;
    line-height: 30rpx;
    color: #999;
    margin-top: 6rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.product-price {
    margin-top: 15rpx;
    line-height: 36rpx;
    height: 36rpx;
    display: flex;
    align-items: center;
}

.product-price view {
    display: inline-block;
}

.product-price .price {
    color: #f23030;
    margin-right: 10rpx;
}

.product-price .bi {
    font-size: 24rpx;
    margin-right: 6rpx;
}

.product-price .big-price {
    font-size: 36rpx;
}

.product-price .small-price {
    font-size: 30rpx;
}

.product-price .coupon {
    background-color: #f23030;
    font-size: 20rpx;
    color: #fff;
    margin-right: 4rpx;
    padding: 3rpx;
    border-radius: 3rpx;
    height: 24rpx;
    line-height: 24rpx;
    vertical-align: middle;
}

.product-comment {
    display: flex;
    align-items: center;
    margin-top: 10rpx;
    line-height: 24rpx;
    overflow: hidden;
}

.product-comment view {
    display: inline-block;
}

.product-comment .tag {
    border: 1rpx solid #f23030;
    color: #f23030;
    font-size: 20rpx;
    padding: 1rpx 6rpx;
    border-radius: 2rpx;
    margin-right: 10rpx;
    height: 24rpx;
    line-height: 24rpx;
}

.product-comment .comment {
    color: #999;
    font-size: 24rpx;
    margin-right: 25rpx;
}

.product-null {
    display: flex;
    flex-direction: column;
    align-items: center;
    overflow: hidden;
    background-color: #f0f2f5;
    padding-bottom: 100rpx;
}

.product-null image {
    height: 160rpx;
}

.product-null .null-icon {
    width: 293rpx;
    height: 175rpx;
    background-image: url(data:image/png;base64,);
    background-size: 293rpx 175rpx;
    background-position: 0 0;
    margin-top: 30%;
    margin-bottom: 40rpx;
}

.product-null .null-info {
    color: #686868;
    font-size: 32rpx;
}

.product-null .null-info2 {
    color: #686868;
    font-size: 28rpx;
}

.product-nomore {
    position: relative;
    height: 50rpx;
    text-align: center;
    margin: 0 20rpx;
    padding: 44rpx 0;
}

.product-nomore::after {
    content: "";
    height: 2rpx;
    width: 100%;
    position: absolute;
    top: 50%;
    left: 0;
    background-color: #cbcbcb;
    transform: scaleY(0.5);
}

.product-nomore-text {
    display: inline-block;
    padding: 0 40rpx;
    background-color: #f0f2f5;
    z-index: 10;
    font-size: 28rpx;
    line-height: 50rpx;
    color: #848689;
    position: relative;
}

.gotop {
    position: fixed;
    bottom: 160rpx;
    right: 60rpx;
    background-image: url(data:image/png;base64,);
    background-size: 200rpx 200rpx;
    width: 72rpx;
    height: 70rpx;
    background-position: 0rpx -79rpx;
}

.search-list-scroll {
    padding-bottom: 100rpx;
}

.slist-cart-button {
    position: absolute;
    background: url(http://commonst.360buyimg.com/common/wx-images/slist-cart-btn.png) no-repeat;
    background-size: 54rpx 54rpx;
    background-position: 10rpx 10rpx;
    width: 74rpx;
    height: 74rpx;
    bottom: 6rpx;
    right: 10rpx;
    display: block;
}

.search-list-tip {
    font-size: 24rpx;
    color: #232326;
    background-color: #f8f8f8;
    width: 100%;
    line-height: 32rpx;
    padding: 14rpx 32rpx;
    box-sizing: border-box;
    word-break: break-all;
}

.search-list-btm-bar {
    display: flex;
    align-items: center;
    justify-content: flex-end;
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 100rpx;
    background-color: #fff;
    border-top: 1px solid #e3e5e9;
}

.checked-info {
    flex: 1;
    padding-left: 20rpx;
}

.checked-info-sum {
    font-size: 28rpx;
    color: #232326;
}

.checked-info-tip {
    font-size: 20rpx;
    color: #848689;
}

.back-cart-btn {
    display: flex;
    width: 218rpx;
    height: 100%;
    align-items: center;
    justify-content: center;
    background-color: #f23030;
    color: #fff;
    font-size: 36rpx;
}

.limit-lines {
    overflow: hidden;
    text-overflow: ellipsis;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    vertical-align: middle;
    display: -webkit-box;
    -webkit-box-pack: center;
    -webkit-box-align: start;
    word-break: break-all;
}

@import "../../__wuBaseWxss__/7.wxss";