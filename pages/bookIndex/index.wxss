page {
    height: 100%;
}

view,text {
    font-family: Arial,Helvetica,sans-serif;
}

.content {
    position: relative;
    background-color: #f7f4f3;
    height: 100%;
    overflow: hidden;
}

.goods-box {
    position: relative;
    padding-top: 24rpx;
    height: 89%;
}

.goods-title {
    padding-bottom: 28rpx;
}

.goods-box .title {
    text-align: center;
    font-size: 28rpx;
    font-weight: bold;
    color: #000;
}

.goods-box .goods-page {
    position: absolute;
    font-size: 22rpx;
    color: #a2a2a2;
    right: 50rpx;
    top: 30rpx;
}

.goods-title .t {
    display: inline-block;
    vertical-align: middle;
}

.goods-title image {
    width: 12rpx;
    height: 26rpx;
    margin: 1rpx 22rpx;
    background-size: 200rpx 200rpx;
    background-image: url(data:image/png;base64,);
    background-position: -42rpx 0rpx;
    vertical-align: middle;
}

.goods-list {
    overflow: hidden;
    padding-bottom: 28rpx;
    height: 95%;
}

.goods-swiper {
    overflow: hidden;
    padding-bottom: 28px;
    height: 95%;
}

.goods-list .wx-swiper-item {
    width: 94%;
}

.goods-list .myleftswiper {
    right: -15%;
}

.goods-list .mycurrswiper {
    width: 94%;
    left: 5%;
}

.goods-list .myrightswiper {
    width: 94%;
    left: -5%;
}

.goods-item {
    margin: 30rpx 30rpx;
    background-color: #ffffff;
    width: 608rpx;
    text-align: center;
    border-radius: 12rpx;
    height: 834rpx;
    position: relative;
    box-shadow: 0 0 5px rgba(0,0,0,.05);
}

.goods-list .goods-item.curr {
    width: 650rpx;
    height: 100%;
    margin: 0 15rpx;
}

.goods-pic,.goods-pic-null {
    width: 300rpx;
    height: 342rpx;
    margin: 50rpx auto 0;
}

.goods-pic-null {
    background-size: 273rpx 391rpx;
    background-image: url(data:image/png;base64,);
}

.good-title {
    max-height: 90rpx;
    line-height: 48rpx;
    font-size: 36rpx;
    font-weight: bold;
    color: #232323;
    max-width: 460rpx;
    margin: 15rpx auto;
    overflow: hidden;
}

.good-author {
    margin-top: 20rpx;
    height: 30rpx;
    line-height: 30rpx;
    overflow: hidden;
    font-size: 26rpx;
    color: #000;
}

.good-author .author {
    max-width: 300rpx;
    display: inline-block;
    overflow: hidden;
    vertical-align: middle;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.good-author-more {
    width: 9rpx;
    height: 16rpx;
    margin-left: 10rpx;
    background-size: 200rpx 200rpx;
    background-image: url(data:image/png;base64,);
    background-position: -65rpx -6rpx;
    vertical-align: middle;
}

.good-desc {
    margin: 20rpx auto 0;
    max-height: 56rpx;
    width: 450rpx;
    overflow: hidden;
    line-height: 28rpx;
    font-size: 22rpx;
    color: #a2a2a2;
}

.good-price {
    padding-top: 20rpx;
    line-height: 40rpx;
    font-size: 40rpx;
    color: #f23030;
}

.good-price .bi {
    display: inline-block;
    font-size: 40rpx;
    margin-right: 6rpx;
}

.good-price .price {
    display: inline-block;
    font-size: 40rpx;
}

.good-addcart {
    position: absolute;
    width: 238rpx;
    height: 72rpx;
    border: 1px solid #f23030;
    border-radius: 6rpx;
    left: 30%;
    bottom: 30rpx;
    font-size: 28rpx;
    color: #f23030;
    display: flex;
    align-items: center;
    justify-content: center;
}

.good-addcart image {
    width: 32rpx;
    height: 30rpx;
    margin-right: 10rpx;
    background-size: 200rpx 200rpx;
    background-image: url(data:image/png;base64,);
    background-position: -41rpx -40rpx;
}

.good-addcart-hover {
    color: #f53218;
}

.good-addcart-hover image {
    background-position: -41rpx -40rpx;
}

.leftTip,.rightTip {
    position: absolute;
    top: 25%;
    width: 28rpx;
    font-size: 28rpx;
    color: #a2a2a2;
}

.leftTip {
    right: -45rpx;
}

.rightTip {
    left: -45rpx;
}

.goods-error .goods-null-icon {
    width: 293rpx;
    height: 175rpx;
    background-image: url(data:image/png;base64,);
    background-size: 293rpx 175rpx;
    background-position: 0 0;
    margin-top: 30%;
    margin-bottom: 40rpx;
}

.goods-error .error-icon {
    width: 147rpx;
    height: 147rpx;
    margin: 180rpx auto 27rpx;
    background-size: 147rpx 147rpx;
    background-image: url(data:image/png;base64,);
    background-position: 0rpx 0rpx;
}

.goods-error .error-title {
    text-align: center;
    padding: 0 50rpx;
    color: #5e6064;
    font-size: 32rpx;
    line-height: 40rpx;
    margin-bottom: 15rpx;
}

.goods-error .error-desc {
    text-align: center;
    padding: 0 50rpx;
    color: #acacac;
    font-size: 26rpx;
    line-height: 36rpx;
    height: 78rpx;
}

.goods-btn {
    width: 320rpx;
    height: 80rpx;
    background-color: #f23030;
    border-radius: 6rpx;
    color: #FFF;
    font-size: 30rpx;
    text-align: center;
    line-height: 80rpx;
    position: absolute;
    left: 27%;
    bottom: 60rpx;
}

.goods-btn-hover {
    background-color: #ce2525;
}

.goods-more .more-icon {
    width: 254rpx;
    height: 230rpx;
    margin: 180rpx auto 27rpx;
    background-size: 254rpx 230rpx;
    background-image: url(data:image/png;base64,);
    background-position: 0rpx 0rpx;
}

.goods-more .more-title {
    font-weight: 600;
    font-size: 40rpx;
    color: #232323;
    margin-top: 30rpx;
}