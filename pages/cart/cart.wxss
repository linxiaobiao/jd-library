    padding: 2px 4px;
    color: #fff;
    font-size: 12px;
    background: #f23030;
    line-height: 12px;
    border-radius: 2px;
    margin: 0 3px;
}

.coupon-inform-expose .inform-expose-icon {
    background: url("https://st.360buyimg.com/order/images/cart5.0/inform-expose-icon.png?t=0510") no-repeat center center;
    background-size: 6px;
    display: inline-block;
    width: 6px;
    height: 10px;
    margin-left: 4px;
    position: relative;
    top: 1px;
}

.coupon-inform-expose:after {
    position: absolute;
    top: -0.5px;
    left: -0.5px;
    width: 200%;
    height: 200%;
    content: '';
    border-top: 1px solid #efe6d4;
    transform: scale(0.5);
    -webkit-transform: scale(0.5);
    transform-origin: top left;
}

.coupon-inform-expose .addAfter:after {
    position: absolute;
    top: -0.5px;
    left: -0.5px;
    width: 200%;
    height: 200%;
    content: '';
    border: 1px solid #efe6d4;
    transform: scale(0.5);
    -webkit-transform: scale(0.5);
    transform-origin: top left;
}

#newBodyHeight {
    overflow-scrolling: touch;
    position: relative;
    overflow: auto;
}

.err-img-hidden {
    visibility: hidden!important;
}

.limit-lines {
    overflow: hidden;
    text-overflow: ellipsis;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    vertical-align: middle;
    display: -webkit-box;
    -webkit-box-pack: center;
    -webkit-box-align: start;
    word-break: break-all;
}

.gift-product-name {
    height: 2.3em;
    overflow: hidden;
    text-overflow: ellipsis;
    line-height: 15px;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    padding: 1px 0;
    font-size: 12px;
    color: #232326;
}

.freezed {
    overflow: hidden;
}

.slide {
    position: relative;
    width: 100%;
    height: 100%;
    z-index: 3;
}

.slide .slide-content {
    position: relative;
    z-index: 2;
    transition: transform 0.25s ease-out;
    transition: transform 0.25s ease-out,-webkit-transform 0.25s ease-out;
    will-change: auto;
    height: 100%;
}

.slide .slide-move {
    will-change: transform;
    transform: translate3d(-118rpx,0,0);
}

.slide .slide-btn-del {
    position: absolute;
    top: 0;
    right: 0;
    display: flex;
    width: 118rpx;
    height: 100%;
    background-color: #f23030;
    color: #fff;
    align-items: center;
    justify-content: center;
}

.promotion {
    position: relative;
    box-sizing: border-box;
    display: inline-block;
    width: 100%;
    line-height: 30rpx;
    padding: 16rpx 80rpx;
    background-color: #fff9f9;
    color: #000;
    font-size: 12px;
    vertical-align: middle;
}

.promotion--pb0 {
    padding-bottom: 0;
}

.promotion::after {
    overflow: hidden;
    position: absolute;
    left: 24rpx;
    top: -5px;
    content: '';
    width: 10px;
    height: 10px;
    border-left: 1rpx solid #fee9ea;
    border-top: 1rpx solid #fee9ea;
    background-color: #fff9f9;
    transform: rotate(45deg);
}

.promotion-label {
    float: left;
    margin-right: 10rpx;
}

.promotion-action {
    float: right;
    margin-right: -60rpx;
}

.promotion-text {
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.pdialog {
    padding: 20rpx 0;
}

.pdialog-head {
    position: relative;
    line-height: 50rpx;
    padding: 0 20rpx;
    color: #848689;
    font-size: 15px;
    text-align: center;
}

.pdialog-head__close {
    position: absolute;
    top: 0;
    bottom: 0;
    right: 24rpx;
    width: 12px;
    height: 12px;
    padding: 5px;
    background: url("https://st.360buyimg.com/order/images/cart5.0/close.icn.png?t=0510") no-repeat center center;
    background-size: 12px 12px;
}

.pdialog-body {
    max-height: 400rpx;
}

.promotion-list {
    padding: 0 20rpx;
}

.promotion-list-item {
    line-height: 40rpx;
    margin: 25rpx 0;
    color: #000;
    font-size: 13px;
}

.promotion-list-item .cart-checkbox {
    float: left;
    margin-top: 0;
    margin-right: 16rpx;
}

.promotion-list-item__text {
    overflow: hidden;
}

.loc-shop-wrap {
    height: 65rpx;
    display: flex;
    align-items: center;
    justify-content: flex-start;
    background-color: #fff9f9;
    font-size: 12px;
    color: #81838e;
    padding-left: 80rpx;
    position: relative;
    box-sizing: border-box;
}

.loc-shop-wrap:after,.loc-shop-wrap:before {
    content: '';
    width: 0;
    height: 0;
    border: solid 13rpx transparent;
    border-bottom: solid 10rpx #ffe4e5;
    border-top-width: 0;
    top: -10rpx;
    left: 29rpx;
    position: absolute;
}

.loc-shop-wrap:after {
    top: -8rpx;
    border-bottom: solid 10rpx #fff9f9;
}

.loc-shop-wrap .loc-shop-lab {
    min-width: 60rpx;
    margin-right: 25rpx;
}

.loc-shop-wrap .loc-shop-info-address {
    padding-right: 20rpx;
}