@import "../../__wuBaseWxss__/10.wxss";

page {
    background: #efeff4;
    height: 100%;
}

.wrap {
    width: 100%;
    min-height: 100%;
    background: #efeff4;
    box-sizing: border-box;
    font-family: Arial,Helvetica,sans-serif;
}

.coupon-list {
    padding-top: 0;
    padding-bottom: 20rpx;
}

.coupon-tip {
    width: 56rpx;
    height: 88rpx;
    display: flex;
    justify-content: flex-end;
    align-items: center;
    padding-right: 32rpx;
}

.coupon-list .right-r {
    padding-right: 0;
}

.coupon-box .shop-coupon-font.coupon-font {
    text-indent: 114rpx;
}

.info-icon {
    display: block;
    background: url(http://commonst.360buyimg.com/common/wx-images/info.png) 0 0 no-repeat;
    background-size: 42rpx 42rpx;
    width: 42rpx;
    height: 42rpx;
}