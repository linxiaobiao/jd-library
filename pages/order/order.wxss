page {
    height: 100%;
}

.page-wrap {
    width: 100%;
    min-height: 100%;
}

view,test,button {
    font-family: Arial,Helvetica,sans-serif;
}

.bd-tb {
    border-top: 1rpx solid #e7e7e7;
    border-bottom: 1rpx solid #e7e7e7;
}

.bd-t {
    border-top: 1rpx solid #e7e7e7;
}

.bd-b {
    border-bottom: 1rpx solid #e7e7e7;
}

.display-none {
    display: none;
}

.my-order-viewport {
    background-color: #efeff4;
    padding-top: 51px;
}

.user-info {
    display: flex;
    align-items: center;
    height: 102rpx;
    margin-top: 20rpx;
    background-color: #fff;
}

.user-info image {
    height: 80rpx;
    width: 80rpx;
    margin-left: 20rpx;
    border-radius: 80rpx;
}

.user-info .user-n {
    font-size: 30rpx;
    color: #000;
    margin-left: 26rpx;
    flex: 1;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    height: 36rpx;
}

.user-info .user-close {
    display: flex;
    flex-direction: column;
    justify-content: center;
    width: 50rpx;
    margin-right: 40rpx;
    margin-left: 8rpx;
}

.user-info .user-close image {
    display: block;
    margin: 0 auto;
    background: url('http://commonst.360buyimg.com/common/wx-images/order/order-sprites.png') no-repeat;
    background-size: 100rpx 100rpx;
    width: 40rpx;
    height: 40rpx;
}

.user-info .user-close view {
    width: 100%;
    text-align: center;
    font-size: 23rpx;
    color: #888;
    margin-top: 9rpx;
}

.user-info .user-back {
    display: flex;
    flex-direction: column;
    justify-content: center;
    width: 50rpx;
    margin-right: 34rpx;
}

.user-back image {
    display: block;
    margin: 0 auto;
    background: url('http://commonst.360buyimg.com/common/wx-images/order/order-sprites.png') no-repeat;
    background-size: 100rpx 100rpx;
    width: 49rpx;
    height: 41rpx;
    background-position: -58rpx 0rpx;
}

.user-back view {
    width: 100%;
    text-align: center;
    font-size: 23rpx;
    color: #888;
    margin-top: 9rpx;
}

.order-ul {
    margin-bottom: 18rpx;
    background-color: #fff;
}

.order-top {
    height: 60rpx;
    display: flex;
    align-items: center;
}

.order-top .icn-pop-shop {
    width: 29rpx;
    height: 26rpx;
    background: url(http://commonst.360buyimg.com/common/wx-images/order/orderdetail/icn.pop.shop.png) 0 0 no-repeat;
    background-size: 29rpx 26rpx;
    margin-left: 20rpx;
}

.order-top .icn-shop {
    background: url('http://commonst.360buyimg.com/common/wx-images/order/order-sprites.png') no-repeat;
    background-size: 100rpx 100rpx;
    width: 39rpx;
    height: 25rpx;
    background-position: 0rpx -48rpx;
    margin-left: 20rpx;
}

.shop-name {
    font-size: 28rpx;
    color: #353535;
    margin-left: 14rpx;
    flex: 1;
}

.order-state {
    font-size: 26rpx;
    color: #000;
    order: 1;
    width: 250rpx;
    text-align: right;
    margin-right: 25rpx;
}

.order-top-car {
    margin-left: 20rpx;
    display: flex;
    padding-top: 27rpx;
    padding-bottom: 20rpx;
}

.order-top-car image {
    background: url('http://commonst.360buyimg.com/common/wx-images/order/order-sprites.png') no-repeat;
    background-size: 50px 50px;
    width: 16px;
    height: 12px;
    background-position: -25px -25px;
}

.order-top-car .car-info {
    font-size: 28rpx;
    margin-left: 20rpx;
    margin-top: -6rpx;
    flex: 1;
    margin-right: 35rpx;
}

.order-li {
    margin-left: 20rpx;
    display: flex;
    align-items: center;
}

.product-img {
    width: 130rpx;
    height: 130rpx;
    overflow: hidden;
    border: 1rpx solid #ececec;
    border-radius: 4rpx;
    margin: 20rpx 22rpx 21rpx 4rpx;
    padding-left: 4rpx;
    padding-top: 4rpx;
}

.product-img image {
    height: 126rpx;
    width: 126rpx;
    background: url('http://commonst.360buyimg.com/common/wx-images/order/orderdetail/default.img.png') 0 0 no-repeat;
    background-size: 126rpx 126rpx;
}

.product-info {
    overflow: hidden;
    color: #353535;
    height: 32px;
    font-size: 14px;
    flex: 1;
    margin-right: 50rpx;
    display: -webkit-box;
    line-height: 16px;
    -webkit-box-orient: vertical;
    text-overflow: ellipsis;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    vertical-align: middle;
    -webkit-box-pack: center;
    -webkit-box-align: start;
    word-break: break-all;
}

.order-bottom {
    height: 95rpx;
    align-items: center;
    display: flex;
}

.order-bottom .text3,.order-bottom .text4 {
    font-size: 30rpx;
}

.order-bottom .text3 {
    color: #353535;
}

.order-bottom .text4 {
    color: #f23030;
}

.text-left {
    display: flex;
    align-items: center;
    width: 370rpx;
    justify-content: flex-start;
    margin-left: 20rpx;
}

.pay-btn {
    height: 100%;
    display: flex;
    justify-content: flex-end;
    align-items: center;
    flex: 1;
}

.button {
    position: relative;
    display: block;
    margin-left: auto;
    margin-right: auto;
    padding-left: 14px;
    padding-right: 14px;
    box-sizing: border-box;
    font-size: 18px;
    text-align: center;
    text-decoration: none;
    line-height: 2.55555556;
    border-radius: 5px;
    -webkit-tap-highlight-color: transparent;
    overflow: hidden;
    color: #000000;
    background-color: #F8F8F8;
}

.pay-btn .button {
    margin-right: 15rpx;
    height: 60rpx;
    font-size: 26rpx;
    color: #f23030;
    border: 1px solid #f23030;
    background-color: #fff;
    line-height: 60rpx;
    box-sizing: border-box;
    border-color: transparent;
}

.pay-btn .button:active {
    background-color: #E3E5E9;
}

.button .message-push-btn {
    background-color: #fff;
    font-size: 26rpx;
}

.button::after {
    content: " ";
    width: 200%;
    height: 200%;
    position: absolute;
    top: 0;
    left: 0;
    transform: scale(0.5);
    transform-origin: 0 0;
    box-sizing: border-box;
    border-radius: 10px;
}

.load {
    display: none;
    align-items: center;
    justify-content: center;
    background-color: #efeff4;
    border: 5rpx solid #efeff4;
    border-radius: 0rpx;
    height: 80rpx;
    font-size: 28rpx;
    color: #888;
    position: static;
}

.bottom-to-top {
    position: fixed;
    bottom: 140rpx;
    right: 20rpx;
    width: 78rpx;
    height: 78rpx;
    z-index: 300;
    transform-origin: 0;
    opacity: 1;
    transform: scale(1,1);
}

.no-more {
    display: none;
    align-items: center;
    justify-content: center;
    color: #888;
    font-size: 28rpx;
    height: 78rpx;
}

.order-commodities {
    width: 100%;
    height: auto;
}

.order-tab {
    display: flex;
    width: 100%;
    height: 51px;
    text-align: center;
    position: fixed;
    left: 0;
    top: 0;
    background-color: #f8f8f8;
    z-index: 10;
}

.tab-item {
    flex: 1;
    font-size: 30rpx;
    line-height: 51px;
    height: 100%;
    color: #848689;
}

.tab-item.checked {
    color: #f23030;
}

.preButtonGray {
    color: gray!important;
    border-color: gray!important;
}