view,text {
    font-family: Arial,Helvetica,sans-serif;
    font-family: "SimHei","Helvetica Neue",Arial,"Droid Sans",sans-serif;
}

.content {
    position: relative;
    background-color: #f7f4f3;
    z-index: 10;
}

.history-box {
    padding-bottom: 20rpx;
    background-color: #FFF;
    border-top: 1rpx solid #e3e5e9;
    font-size: 28rpx;
    color: #232326;
}

.history-title {
    height: 60rpx;
    line-height: 60rpx;
    padding-left: 30rpx;
    font-weight: 700;
    border-bottom: 1rpx solid #e3e5e9;
}

.history-item {
    height: 80rpx;
    line-height: 80rpx;
    border-bottom: 1rpx solid #e3e5e9;
    padding-left: 30rpx;
    position: relative;
    overflow: hidden;
}

.history-item .txt {
    position: absolute;
    background-color: #fff;
    width: 100%;
    left: 30rpx;
    display: inline-block;
    z-index: 100;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    padding-right: 50rpx;
    box-sizing: border-box;
}

.history-item .del {
    float: right;
    width: 60rpx;
    z-index: 9;
    padding: 0 30rpx;
    background-color: #f23030;
    color: #FFF;
}

.clear-btn {
    display: flex;
    justify-content: center;
    align-items: center;
    margin: 45rpx auto;
    width: 452rpx;
    height: 70rpx;
    border: 1rpx solid #bfc0c2;
    border-radius: 6rpx;
    color: #535353;
}

.clear-btn-hover {
    border: 1rpx solid #848689;
    background-color: #e3e5e9;
}

.clear-btn image {
    width: 24rpx;
    height: 26rpx;
    margin-right: 12rpx;
    background-image: url(data:image/png;base64,);
    background-size: 200rpx 200rpx;
    background-position: -119rpx -43rpx;
}

.filter-box {
    width: 750rpx;
    height: 72rpx;
    color: #686868;
    font-size: 30rpx;
    background-color: #fff;
    border-bottom: 1rpx solid #e3e5e9;
}

.filter-box-top {
    position: fixed;
    top: 0;
    left: 0;
    border-top: 1rpx solid #e3e5e9;
}

.filter-box .filter-tab {
    width: 25%;
    float: left;
    text-align: center;
    line-height: 72rpx;
    position: relative;
}

.filter-tab .price-icon {
    position: absolute;
    right: 34rpx;
    top: 22rpx;
    background-image: url(data:image/png;base64,);
    background-size: 200rpx 200rpx;
    width: 18rpx;
    height: 26rpx;
    background-position: -134rpx 0rpx;
}

.filter-tab .price-icon.up {
    background-position: -109rpx 0rpx;
}

.filter-tab .price-icon.down {
    background-position: -83rpx 0rpx;
}

.filter-tab .choose-icon {
    position: absolute;
    right: 34rpx;
    top: 24rpx;
    background-image: url(data:image/png;base64,);
    background-size: 200rpx 200rpx;
    width: 24rpx;
    height: 25rpx;
    background-position: -87rpx -43rpx;
}

.filter-box .curr {
    color: #f23030;
}

.product-list {
    background-color: #fff;
}

.product-list-top {
    margin-top: 76rpx;
}

.product-item {
    width: 100%;
    height: 256rpx;
}

.product-image {
    width: 200rpx;
    height: 200rpx;
    float: left;
    margin-top: 6rpx;
    margin-left: 10rpx;
    position: relative;
}

.product-image .pimage {
    background-size: 211rpx 210rpx;
    background-image: url(data:image/png;base64,);
}

.product-image .product-ext {
    position: absolute;
    width: 100%;
    height: 30rpx;
    line-height: 30rpx;
    bottom: 0;
    left: 0;
    font-size: 24rpx;
    text-align: center;
    background-color: rgba(0,0,0,0.6);
    color: #FFF;
}

.product-image image {
    height: 200rpx;
    max-width: 200rpx;
}

.product-info {
    float: left;
    width: 470rpx;
    height: 217rpx;
    margin-left: 20rpx;
    padding-top: 15rpx;
    border-bottom: 1rpx solid #e3e5e9;
    padding-right: 50rpx;
}

.product-title {
    font-size: 28rpx;
    color: #232326;
    overflow: hidden;
    width: 470rpx;
    height: 40rpx;
    line-height: 40rpx;
}

.product-recommend {
    font-size: 22rpx;
    color: #999;
    height: 30rpx;
    line-height: 30rpx;
    margin-top: 10rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.product-author {
    font-size: 22rpx;
    height: 30rpx;
    line-height: 30rpx;
    color: #999;
    margin-top: 6rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.product-price {
    margin-top: 15rpx;
    line-height: 36rpx;
    height: 36rpx;
}

.product-price view {
    display: inline-block;
}

.product-price .price {
    color: #f23030;
    margin-right: 10rpx;
}

.product-price .bi {
    font-size: 24rpx;
    margin-right: 6rpx;
}

.product-price .big-price {
    font-size: 36rpx;
}

.product-price .small-price {
    font-size: 30rpx;
}

.product-price .coupon {
    background-color: #f23030;
    font-size: 20rpx;
    color: #fff;
    margin-right: 4rpx;
    padding: 3rpx;
    border-radius: 3rpx;
    height: 24rpx;
    line-height: 24rpx;
    vertical-align: middle;
}

.product-comment {
    margin-top: 10rpx;
    margin-bottom: 9rpx;
    height: 36rpx;
    line-height: 22rpx;
    overflow: hidden;
}

.product-comment view {
    display: inline-block;
}

.product-comment .tag {
    border: 1rpx solid #f23030;
    color: #f23030;
    font-size: 20rpx;
    padding: 1rpx 6rpx;
    border-radius: 2rpx;
    margin-right: 10rpx;
    height: 24rpx;
    line-height: 24rpx;
}

.product-comment .comment {
    color: #999;
    font-size: 24rpx;
    margin-right: 25rpx;
}

.product-null {
    display: flex;
    flex-direction: column;
    align-items: center;
    overflow: hidden;
    background-color: #f0f2f5;
}

.product-null image {
    height: 160rpx;
}

.product-null .null-icon {
    width: 293rpx;
    height: 175rpx;
    background-image: url(data:image/png;base64,);
    background-size: 293rpx 175rpx;
    background-position: 0 0;
    margin-top: 30%;
    margin-bottom: 40rpx;
}

.product-null .null-info {
    color: #686868;
    font-size: 32rpx;
}

.product-null .null-info2 {
    color: #686868;
    font-size: 28rpx;
}

.product-nomore {
    padding: 20rpx;
    font-size: 24rpx;
    color: #999;
    text-align: center;
}

.gotop {
    position: fixed;
    bottom: 60rpx;
    right: 60rpx;
    background-image: url(data:image/png;base64,);
    background-size: 200rpx 200rpx;
    width: 72rpx;
    height: 70rpx;
    background-position: 0rpx -79rpx;
}