.coupon-list {
    width: 100%;
    padding: 0 20rpx 120rpx 20rpx;
    box-sizing: border-box;
}

.coupon-box {
    width: 100%;
    height: 200rpx;
    background: #fff;
    margin-top: 20rpx;
    box-sizing: border-box;
    border: 1rpx solid #f2f2f2;
    border-left: none;
    display: flex;
    border-radius: 6rpx;
}

.coupon-box .m-box {
    width: 100%;
    height: 110rpx;
    display: block;
    display: -webkit-flex;
    display: flex;
    justify-content: center;
}

.coupon-box .f30 {
    font-size: 36rpx;
    height: 110rpx;
    line-height: 186rpx;
    display: block;
    overflow: hidden;
}

.coupon-box .f36 {
    font-size: 42rpx;
    height: 110rpx;
    line-height: 160rpx;
    display: block;
    overflow: hidden;
}

.coupon-box .f76 {
    font-size: 88rpx;
    height: 110rpx;
    line-height: 152rpx;
    display: block;
    overflow: hidden;
}

.coupon-box .n-m {
    width: 100%;
    display: block;
    display: -webkit-flex;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 24rpx;
    height: 84rpx;
}

.coupon-box .right-box {
    height: 100%;
    flex: 1;
    display: flex;
}

.coupon-box .right-l {
    width: 320rpx;
    box-sizing: border-box;
    padding-left: 20rpx;
}

.coupon-box .right-r {
    flex: 1;
    padding-right: 32rpx;
    box-sizing: border-box;
    display: flex;
    justify-content: flex-end;
    align-items: center;
}

.coupon-box .check {
    width: 36rpx;
    height: 36rpx;
    background: url(data:image/png;base64,);
    background-size: 100% 100%;
}

.coupon-box .checked {
    width: 36rpx;
    height: 36rpx;
    background: url(data:image/png;base64,);
    background-size: 100% 100%;
}

.coupon-l {
    position: relative;
    height: 68rpx;
    font-size: 0;
    margin: 22rpx 0 6rpx;
}

.coupon-bg {
    font-size: 20rpx;
    line-height: 30rpx;
    color: #fff;
    height: 30rpx;
    padding: 0 15rpx;
    border-radius: 3rpx;
    display: inline-block;
    margin-right: 10rpx;
    width: auto;
    vertical-align: top;
    position: absolute;
    top: 4rpx;
}

.coupon-bg::before {
    content: "";
    display: inline-block;
    vertical-align: middle;
    width: 0;
    height: 100%;
    margin-top: 4rpx;
}

.coupon-font {
    display: inline;
    font-size: 30rpx;
    line-height: 38rpx;
    text-indent: 92rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.overlay-wrap {
    width: 100%;
    height: 30rpx;
}

.use-t {
    font-size: 22rpx;
    color: #888888;
    line-height: 22rpx;
    height: 22rpx;
    display: flex;
    align-items: center;
    margin: 22rpx 0 26rpx;
}

.blue .left-box {
    width: 250rpx;
    height: 100%;
    background: linear-gradient(135deg,#5293d5,#6579dc);
    position: relative;
    color: #fff;
    font: 30rpx;
}

.blue .r {
    position: absolute;
    width: 5rpx;
    height: 100%;
    background: url(data:image/png;base64,);
    background-repeat: repeat-y;
    background-size: 5rpx 11rpx;
    left: 0;
    top: 0;
}

.blue .coupon-bg {
