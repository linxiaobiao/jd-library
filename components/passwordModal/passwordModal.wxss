.wallets-password {
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.6);
}

.wallets-password .input-content-wrap .top {
    display: flex;
    align-items: center;
    height: 90rpx;
    border-bottom: 2rpx solid #ddd;
    justify-content: space-around;
}

.wallets-password .input-content-wrap .top .close {
    font-size: 44rpx;
    color: #999;
    font-weight: 100;
}

.wallets-password .input-content-wrap .top .forget {
    color: #00a2ff;
    font-size: 22rpx;
}

.wallets-password .input-content-wrap .input-password-wrap {
    display: flex;
    align-items: center;
    justify-content: center;
    margin: 0 28rpx;
    border: 1rpx solid #dcdcdc;
    border-radius: 6rpx;
}

.wallets-password .input-content-wrap .input-password-wrap .password-dot {
    color: #000;
    border-left: 1rpx solid #dcdcdc;
    width: 96rpx;
    height: 86rpx;
    line-height: 46px;
    text-align: center;
}

.wallets-password .input-content-wrap .password-dot:nth-child(1) {
    border-left: none;
}

.wallets-password .input-content-wrap .long-password-box {
    position: relative;
    width: 576rpx;
    height: 86rpx;
    border: 1rpx solid #bfbfbf;
    border-radius: 6rpx;
    padding-left: 14rpx;
}

.long-password-box-hint {
    position: absolute;
    font-size: 22rpx;
    top: 20rpx;
    color: #bfbfbf;
}

.wallets-password .input-content-wrap .long-password-dot {
    float: left;
    font-size: 20rpx;
    background: #000;
    box-sizing: border-box;
    width: 10rpx;
    height: 10rpx;
    margin-right: 10rpx;
    margin-top: 32rpx;
    border-radius: 10rpx;
}

.wallets-password .input-content-wrap .input-password-wrap .password-dot i {
    display: inline-block;
    background: #000;
    border-radius: 50%;
    width: 20rpx;
    height: 20rpx;
}

.wallets-password .input-content {
    position: absolute;
    opacity: 0;
    left: -1000rpx;
    top: 600rpx;
    background: #f56;
    z-index: -999;
    border: 1rpx solid red;
}

.wallets-password .input-content.active {
    z-index: -99;
}

.coupon-modal {
    width: 630rpx;
    height: auto;
    background: #fff;
    z-index: 300;
    margin: auto;
    border-radius: 14rpx;
    transform: translate(0%,50%);
    font-family: Arial,Helvetica,sans-serif;
    overflow: hidden;
}

.coupon-modal-title {
    font-size: 36rpx;
    padding: 44rpx 0 38rpx 0rpx;
    text-align: center;
    color: #232326;
}

.coupon-modal-hint {
    font-size: 28rpx;
    color: #686868;
    padding: 0 28rpx;
    margin-bottom: 38rpx;
}

.coupon-modal-passWorld {
    padding: 0 28rpx;
}

.coupon-modal-passWorld input {
    font-size: 14px;
    height: 86rpx;
    color: #808080;
    padding-left: 10rpx;
    border: 1rpx solid #dcdcdc;
    border-radius: 6rpx;
}

.coupon-modal-passWorld .placeholder {
    color: #bfbfbf;
}

.coupon-modal-button {
    height: 88rpx;
    line-height: 88rpx;
    font-size: 32rpx;
    overflow: hidden;
    vertical-align: middle;
}

.coupon-modal-button .public-style {
    width: 50%;
    height: 100%;
    text-align: center;
}

.coupon-modal-button-cancel {
    border-top: 1rpx solid #dcdcdc;
    color: #4d4d4d;
    float: left;
}

.coupon-modal-button-notarize {
    float: right;
    background: #f23030;
    color: #fff;
    border-top: 1rpx solid #f23030;
}

.coupon-modal-button-notarize button {
    background: #f23030;
    color: #fff;
    font-size: 32rpx;
}

.coupon-modal-mask {
    width: 100%;
    height: 100%;
    position: fixed;
    background: rgba(0,0,0,0.5);
    left: 0;
    top: 0;
}

.modal-block {
    display: block;
}

.modal-none {
    display: none;
}

.modal-err-msg {
    align-items: center;
    font-size: 24rpx;
    color: #e64340;
}

.coupon-modal-removePassword {
    overflow: hidden;
    height: 72rpx;
    line-height: 72rpx;
    padding: 0 28rpx;
}

.modal-removePassword-button {
    font-size: 24rpx;
    color: #bfbfbf;
}

.left {
    float: left;
}

.right {
    float: right;
}