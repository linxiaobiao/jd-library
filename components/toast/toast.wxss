.shade-floor {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1000;
    display: flex;
    align-items: center;
    justify-content: center;
}

.message-box {
    min-width: 46.875%;
    max-width: 71%;
    background: rgba(0,0,0,0.6);
    border-radius: 8px;
    text-align: center;
    padding: 18px 15px;
    font-size: 0px;
}

.message-box .message-box-icon {
    height: 37px;
    display: flex;
    margin-bottom: 18px;
    justify-content: center;
}

.message-icon {
    background: url("data:image/png;base64,") no-repeat;
    background-size: 100px 100px;
}

.message-box .message-box-content {
    font-size: 16px;
    line-height: 20px;
    color: #fff;
    word-break: break-all;
}

.success-icon {
    display: inline-block;
    height: 37px;
    width: 37px;
    background-position: 0px 0px;
}

.error-icon {
    display: inline-block;
    height: 37px;
    width: 37px;
    background-position: 0px -38px;
}