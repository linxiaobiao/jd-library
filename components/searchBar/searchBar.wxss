.search-box {
    font-size: 26rpx;
    position: relative;
    width: 650rpx;
    padding: 22rpx 50rpx 20rpx;
}

.search-box .search-icon {
    position: absolute;
    left: 70rpx;
    top: 34rpx;
    height: 36rpx;
    width: 36rpx;
    background-size: 200rpx 200rpx;
    background-image: url(data:image/png;base64,);
    background-position: 0rpx 0rpx;
}

.search-box input {
    border-radius: 62rpx;
    color: #232326;
    background-color: #ffffff;
    height: 62rpx;
    padding-left: 75rpx;
    padding-right: 20rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.search-box .placeholder-text {
    color: #9b9b9b;
}

.search-box .mask {
    position: absolute;
    height: 62rpx;
    width: 650rpx;
    top: 22rpx;
    z-index: 100;
    opacity: 0.1;
    background-color: #ddd;
}

.search-box .clear-icon {
    display: none;
    position: absolute;
    z-index: 999;
    right: 120rpx;
    top: 34rpx;
    height: 40rpx;
    width: 40rpx;
    background-size: 200rpx 200rpx;
    background-image: url(data:image/png;base64,);
    background-position: -156rpx 0rpx;
}

.search-box .search-btn {
    display: none;
    float: right;
    margin-right: -82rpx;
    margin-top: -45rpx;
    color: #686868;
    font-size: 28rpx;
}

.search-box.focus {
    padding-right: 112rpx;
    background-color: #FFF;
    width: 588rpx;
}

.focus .search-btn {
    display: block;
}

.focus input {
    background-color: #f0f2f5;
    color: #232326;
    padding-right: 60rpx;
}

.focus .clear-icon {
    display: block;
}

.whitebg {
    background-color: #FFF;
}

.whitebg input {
    background-color: #f0f2f5;
    color: #232326;
}

.whitebg .placeholder-text {
    color: #bfbfbf;
}

.whitebg {
    border-top: 1px solid #e3e5e9;
    border-bottom: 1px solid #e3e5e9;
}