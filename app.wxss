@import "wxParse/wxParse.wxss";

@import "components/toast/toast.wxss";

@import "components/noData/noData.wxss";

text,view {
    font-family: Arial,Helvetica,sans-serif;
}

.jd-red-bg {
    background-color: #f23030!important;
}

.jd-red {
    color: #f23030!important;
}

.jd-red-bd {
    border-color: #f23030!important;
}

.bottom-to-top {
    position: fixed;
    bottom: 216rpx;
    right: 20rpx;
    width: 78rpx;
    height: 78rpx;
    z-index: 300;
    transform-origin: 0;
    opacity: 1;
    transform: scale(1,1);
}

.message-push-btn {
    padding: 0;
    border-radius: 0;
    overflow: inherit;
    text-align: inherit;
}

.message-push-btn:after {
    border: none;
    border-radius: 0;
}

.getuserinfo-btn {
    padding: 0;
    border-radius: 0;
    overflow: inherit;
    text-align: inherit;
    background-color: transparent;
}

.getuserinfo-btn:after {
    border: none;
    border-radius: 0;
}

.one-ellipsis {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

@import "pages/bookTemplate/bookTemplate.wxss";